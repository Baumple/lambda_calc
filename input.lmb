# SUCC und pred abstractions
succ    <- λn.λf.λx.(
    f(n f x)
)
pred    <- λn.λf.λx.(
    n (λg.λh.(h (g f))) (λu.x) (λu.u)
)

compose <- λf.λg.λx.(
    f (g x)
)

zero   <- (λf.λx.x)

(compose pred succ zero)
